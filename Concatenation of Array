class Solution {
    public int[] getConcatenation(int[] nums) {
      int l=nums.length;
      int j=0;
      int n=2*l;
      int[] num=new int[n];
      for(int i=0;i<n;i++)
      {
        if(j==l)
        {
           j=0;
        }
        num[i]=nums[j++];
      }   
      return num; 
    }
}
